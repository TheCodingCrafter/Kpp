#include <iostream>
#include <vector>

using std

func vector<double> add(vector<double> arr1, vector<double>, arr2)
    vector<double> tmp
    for int i = 0; i < arr1.size(); i++
        tmp.push_back(arr1[i] + arr2[i])
    return tmp
end

func int main()
    vector<double> m = add(vector<double>{1, 2, 3}, vector<double>{4, 5, 6})
    for int i = 0; i < m.size(); i++
        cout << m[i] << endl
    end
end